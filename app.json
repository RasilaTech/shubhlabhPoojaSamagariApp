{
  "expo": {
    "name": "ShubhlabhPoojaSamagari",
    "slug": "shubhlabhPoojaSamagariApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/playstore-icon.png",
    "scheme": "shubhlabhpoojasamagariapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
"splash": {
  "image": "./assets/images/splash-logo.png",
  "resizeMode": "contain",
  "backgroundColor": "#3e0f11",
  "android": {
    "image": "./assets/images/splash-logo.png"
  },
  "ios": {
    "image": "./assets/images/splash-logo.png"
  }
},
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shubhlabh.poojasamagri",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need access to your location to provide you with a list of nearby stores and services."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDnxdR14ni_GJGMOClN8gScZmbOH88i8g0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore-icon.png",
        "backgroundColor": "#3e0f11"
      },
      "proguardObfuscate": true,
      "enableProguardInReleaseBuilds": true,
      "minifyEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDnxdR14ni_GJGMOClN8gScZmbOH88i8g0"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.shubhlabh.poojasamagri",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ic_launcher-web.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-secure-store",
       [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "enableProguardInReleaseBuilds":true,
            "enableShrinkResourcesInReleaseBuilds":true
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0a7c6064-dbb8-45ab-9498-8afa6776412a"
      }
    }
  }
}
